<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>ant-typesetting</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>ant-typesetting</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>ant-typesetting</label>
                    <location>.</location>
                    <encoding>windows-1252</encoding>
                </source-folder>
                <source-folder>
                    <label>src\java</label>
                    <type>java</type>
                    <location>src/java</location>
                    <encoding>windows-1252</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>build</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="test">
                    <target>test</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>build</target>
                </action>
                <action name="debug">
                    <target>debug</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>build/classes</location>
                <build-target>build</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src\java</label>
                        <location>src/java</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="test"/>
                    <ide-action name="debug"/>
                    <action>
                        <label>Debug Project</label>
                        <target>debug</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/3">
            <compilation-unit>
                <package-root>src/java</package-root>
                <classpath mode="compile">lib/ant.jar</classpath>
                <built-to>build/classes</built-to>
                <source-level>1.7</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
